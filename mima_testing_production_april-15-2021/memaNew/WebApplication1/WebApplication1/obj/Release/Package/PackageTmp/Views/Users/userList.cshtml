
@model IEnumerable<WebApplication1.Models.User>




<table class="table table-bordered" id="myTable">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Role</th>
            @*<th>Description</th>*@
            <th>Action</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {

            <tr>
                <td>@item.User_ID</td>
                <td>@item.User_Name</td>
                <td>@item.User_Role</td>
                @*<td>@item.Password</td>*@
                <td>
                    <button  class="btn btn-success addedituser" id="@item.User_ID">edit</button>
                    @*<button class="btn btn-sm btn-danger" id="@item.User_ID">delete</button>*@

                </td>

            </tr>
        }



    </tbody>
</table>

