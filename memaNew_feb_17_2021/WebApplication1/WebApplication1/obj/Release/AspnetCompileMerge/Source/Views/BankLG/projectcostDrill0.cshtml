
@model IEnumerable<WebApplication1.Models.Usp_Project_Cost_GL_Result>

<table class="table table-bordered drillTable" id="drill0Table">
    <thead>
        <tr>
            <th>Project</th>
            <th>Cost</th>

        </tr>
    </thead>
    <tbody>
       
        @foreach (var item in Model)
        {
           
            <tr>
                <td class="drill1" id="@item.Project"><a href="javascript:;">@item.Project</a></td>


                <td>@String.Format("{0:#,0}", (Convert.ToInt32(item.Cost) == null ? 0 : Convert.ToInt32(item.Cost) ))</td>

            </tr>
                    }
    </tbody>
</table>
